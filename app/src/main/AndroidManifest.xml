<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.michaelwang.capitaltogo">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeScreenActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".LoginActivity"></activity>
        <activity android:name=".RequestCashActivity"></activity>
        <activity android:name=".CodeActivity"></activity>
        <activity android:name=".TransactionSuccessActivity"></activity>
        <activity android:name=".RegisterPrintActivity"></activity>
        <activity android:name=".RegisterSuccessActivity"></activity>
    </application>

</manifest>